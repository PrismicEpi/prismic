  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "LoginRequest": {
        "description": "",
        "required": [
          "authorization"
        ],
        "type": "object",
        "properties": {
          "authorization": {
            "description": "Bearer token in Authorization header",
            "type": "string"
          }
        }
      },
      "LoginResponse": {
        "description": "",
        "type": "object",
        "properties": {
          "userName": {
            "description": "The user's name.",
            "type": "string"
          },
          "email": {
            "description": "The user's email.",
            "type": "string"
          },
          "avatar": {
            "description": "URL to the user's avatar.",
            "type": "string"
          }
        }
      },
      "Experiment": {
        "description": "",
        "required": [
          "experiment_id",
          "user_email",
          "input_txt",
          "experiment_date_start"
        ],
        "type": "object",
        "properties": {
          "experiment_id": {
            "description": "Experiment UUID",
            "type": "string"
          },
          "user_email": {
            "description": "User email",
            "type": "string"
          },
          "input_txt": {
            "description": "Input text",
            "type": "string"
          },
          "experiment_date_start": {
            "description": "Start date (ISO 8601 format)",
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "UpdateExperimentRequest": {
        "description": "",
        "required": [
          "experiment_id",
          "updateFields"
        ],
        "type": "object",
        "properties": {
          "experiment_id": {
            "description": "ID of the experiment to update",
            "type": "string"
          },
          "updateFields": {
            "description": "Fields to update (like status, result_txt, etc.)",
            "type": "object"
          }
        }
      }
    }
  },
